    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "CUDA_VISIBLE_DEVICES": "0"
            },
        },
        {
            "name": "Test CLI",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/cggp/cli.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}:${workspaceFolder}/cggp/:${env:PYTHONPATH}",
                "CUDA_DEVICE_ORDER": "PCI_BUS_ID",
                "CUDA_VISIBLE_DEVICES": "3",
            },
            "justMyCode": false,
            "args": [
                "--dataset",
                "elevators",
                "--jit",
                "train-cggp-adam",
                "--num-iterations",
                "100",
                "--num-inducing-points",
                "1000",
                "--batch-size",
                "2000",
            ],
        },
        {
            "name": "Test CLI covertree v2",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/cggp/paper_cli_v2.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}:${workspaceFolder}/cggp/:${env:PYTHONPATH}",
                "CUDA_DEVICE_ORDER": "PCI_BUS_ID",
                "CUDA_VISIBLE_DEVICES": "3",
            },
            "justMyCode": false,
            "args": [
                "--dataset",
                "snelson1d",
                "--model-class",
                "sgpr",
                "--seed",
                "111",
                "--jit",
                "covertree",
                "--spatial-resolution",
                "0.5",
                "train-adam",
                "--num-iterations",
                "2",
                "--learning-rate",
                "0.1",
                "--batch-size",
                "2000",
                "--no-tip",
                "--no-tensorboard"
            ],
        },
        {
            "name": "Test CLI kmeans v2",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/cggp/paper_cli_v2.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}:${workspaceFolder}/cggp/:${env:PYTHONPATH}",
                "CUDA_DEVICE_ORDER": "PCI_BUS_ID",
                "CUDA_VISIBLE_DEVICES": "3",
            },
            "justMyCode": false,
            "args": [
                "--dataset",
                "elevators",
                "--model-class",
                "sgpr",
                "--seed",
                "111",
                "--jit",
                "kmeans",
                "--max-num-ip",
                "100",
                "train-adam",
                "--num-iterations",
                "2",
                "--learning-rate",
                "0.1",
                "--batch-size",
                "2000",
                "--no-tip",
                "--no-tensorboard"
            ],
        },
        {
            "name": "Test CLI oips v2",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/cggp/paper_cli_v2.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}:${workspaceFolder}/cggp/:${env:PYTHONPATH}",
                "CUDA_DEVICE_ORDER": "PCI_BUS_ID",
                "CUDA_VISIBLE_DEVICES": "0",
            },
            "justMyCode": false,
            "args": [
                "--dataset",
                "elevators",
                "--model-class",
                "sgpr",
                "--seed",
                "111",
                "--no-jit",
                "oips",
                "--rho",
                "0.5",
                "--max-num-ip",
                "100",
                "train-adam",
                "--num-iterations",
                "2",
                "--learning-rate",
                "0.1",
                "--batch-size",
                "2000",
                "--no-tip",
                "--no-tensorboard"
            ],
        },
        {
            "name": "Test GPR",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/cggp/paper_cli_gpr.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}:${workspaceFolder}/cggp/:${env:PYTHONPATH}",
                "CUDA_DEVICE_ORDER": "PCI_BUS_ID",
                "CUDA_VISIBLE_DEVICES": "3",
            },
            "justMyCode": false,
            "args": [
                "--dataset",
                "elevators",
                "--seed",
                "111",
                "--no-jit",
                "--num-iterations",
                "2",
                "--test-batch-size",
                "2000",
                "--no-tensorboard"
            ],
        },
        {
            "name": "Test Exp-1",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/cggp/paper_cli_uci.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}:${workspaceFolder}/cggp/:${env:PYTHONPATH}",
                "CUDA_DEVICE_ORDER": "PCI_BUS_ID",
                "CUDA_VISIBLE_DEVICES": "3",
            },
            "justMyCode": false,
            "args": [
                "--model-class",
                "sgpr",
                "--precision",
                "fp64",
                "--jitter",
                "1e-6",
                "--config-dir",
                "${workspaceFolder}/cggp/logs/gpr/${input:dataset}/gpr_matern32_i1000/111",
                "--no-jit",
                "${input:selection}",
                "-m",
                "2000",
                "compute-metrics",
            ],
        },
    ],
    "inputs": [
        {
            "type": "pickString",
            "id": "dataset",
            "description": "Datasets",
            "default": "elevators",
            "options": [
                "bike",
                "elevators"
            ]
        },
        {
            "type": "pickString",
            "id": "selection",
            "description": "Selection",
            "default": "uniform",
            "options": [
                "uniform",
                "kmeans",
            ]
        },
        {
            "type": "promptString",
            "id": "path",
            "description": "Apply JIT",
            "default": "${workspaceFolder}/experiments/logs-fp64/elevators/cglb_m4000-mt32-i1000-adam/*/traces.logs.npy",
        }
    ]
}