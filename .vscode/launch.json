{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "CUDA_VISIBLE_DEVICES": "0"
            },
        },
        {
            "name": "Test CLI",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/cggp/cli.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}:${workspaceFolder}/cggp/:${env:PYTHONPATH}",
                "CUDA_DEVICE_ORDER": "PCI_BUS_ID",
                "CUDA_VISIBLE_DEVICES": "3",
            },
            "justMyCode": false,
            "args": [
                "--dataset",
                "elevators",
                "--jit",
                "train-cggp-adam",
                "--num-iterations",
                "100",
                "--num-inducing-points",
                "1000",
                "--batch-size",
                "2000",
            ],
        },
        {
            "name": "Test CLI v2",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/cggp/paper_cli_v2.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}:${workspaceFolder}/cggp/:${env:PYTHONPATH}",
                "CUDA_DEVICE_ORDER": "PCI_BUS_ID",
                "CUDA_VISIBLE_DEVICES": "3",
            },
            "justMyCode": false,
            "args": [
                "--dataset",
                "snelson1d",
                "--model-class",
                "sgpr",
                "--seed",
                "111",
                "--jit",
                "covertree",
                "--spatial-resolution",
                "0.5",
                "train-adam",
                "--num-iterations",
                "2",
                "--learning-rate",
                "0.1",
                "--batch-size",
                "2000",
                "--no-tip",
                "--no-tensorboard"
            ],
        },
    ]
}